/* Acciones prohibidas */
set MolinetesAlfa = {east.salir, west.entrar}

DIRECTOR =  ( abrir  -> DIRECTOR
			| cerrar -> DIRECTOR
			) + MolinetesAlfa.

MOLINETE  = ( habilitar    -> M_HAB ),
M_HAB     = ( entrar       -> M_HAB
			| salir        -> M_HAB
			| deshabilitar -> MOLINETE
			).

||MUSEO = ( DIRECTOR || {east,west}:MOLINETE )
			/{abrir/{east,west}.habilitar, cerrar/east.deshabilitar}
			\{west.deshabilitar}.
